// setup string to be drawn
var str = "";

var u_first = u_current_last - u_max_lines;
if(u_first &lt; 0) { u_first = 0; }

for(var u = u_current_last; u &gt;= u_first; u--)
    {
    str = list_s_messages[|u] + "#________________________________#" + str;
    }

// set here for the string_height_ext calculations
draw_set_font(fnt_arial12)
draw_set_halign(fa_left);
draw_set_valign(fa_top);

var f_padding = 8;
    

var f_width = window_get_width();
var f_str_width = f_width - (f_padding * 2);
var f_str_height = string_height_ext(str, 15, f_str_width);
var f_height = f_padding /*bottom*/ + f_str_height + f_padding /*top*/;

var f_orig_x = 0;
var f_orig_y = window_get_height() - f_height;

var f_str_orig_x = f_padding;
var f_str_orig_y = f_orig_y + f_padding;

// BG
draw_set_alpha(.75);
draw_set_color(c_dkgray)

draw_rectangle(f_orig_x, f_orig_y, f_orig_x + f_width, f_orig_y + f_height, false);

draw_set_alpha(1);
draw_set_color(c_gray)

draw_line(f_orig_x, f_orig_y, f_orig_x + f_width, f_orig_y);

// Text
draw_set_alpha(1);
draw_set_color(c_white);
draw_text_ext(f_str_orig_x, f_str_orig_y, str, 15, f_str_width);
</string>
